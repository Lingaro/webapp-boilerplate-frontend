{
    "extends": "tslint:recommended",
    "rules": {
        "ordered-imports": false,
        "interface-name": [
            "never-prefix"
        ],
        "object-literal-sort-keys": false,
        "array-type": [
            "array-simple"
        ],
        "arrow-parens": false,
        "quotemark": [
            false
        ],
        "no-string-literal": false,
        "only-arrow-functions": [
            "allow-named-functions"
        ],
        "no-var-requires": false,
        "no-empty": false,
        "no-var-keyword": true
    }
}